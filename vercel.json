{
	"version": 2,
	"builds": [
		{
			"src": "index.js",
			"use": "@vercel/node"
		},
		{
			"src": "css/base.css",
			"use": "@vercel/static"
		  },
		  {
			"src": "index.html",
			"use": "@vercel/static"
		  }
	],
	
	"routes": [{"handle": "filesystem"},
	{
			"src": "/(.*)",
			"dest": "/index.js"
		}
	]
	
}